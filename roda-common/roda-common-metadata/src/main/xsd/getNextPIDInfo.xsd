<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="pidList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="pid" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="pid" type="xs:string"/>
</xs:schema>

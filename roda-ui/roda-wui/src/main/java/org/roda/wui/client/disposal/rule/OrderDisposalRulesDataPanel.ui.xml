<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
>

    <ui:with field='messages' type='config.i18n.client.ClientMessages'/>

    <g:FlowPanel styleName="wui-representation-information-network " addStyleNames="wrapper skip_padding">
        <g:FlowPanel addStyleNames="row full_width skip_padding">
            <g:FlowPanel addStyleNames="row full_width skip_padding">
                <g:FlowPanel addStyleNames="col_10">
                    <g:FlowPanel ui:field="orderDisposalRulesTablePanel">
                    </g:FlowPanel>
                </g:FlowPanel>
                <g:FlowPanel addStyleNames="col_2 orderButtons">
                    <g:FlowPanel addStyleNames="sticky-flow">
                        <g:FlowPanel addStyleNames="sticky-padding">
                            <g:FlowPanel addStyleNames="alignButtonsPanel"
                                         ui:field="orderButtonsPanel"></g:FlowPanel>
                        </g:FlowPanel>
                    </g:FlowPanel>
                </g:FlowPanel>
            </g:FlowPanel>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:org.roda.wui.client.main"
             xmlns:wcag="urn:import:org.roda.wui.common.client.widgets.wcag"
             xmlns:common="urn:import:org.roda.wui.client.common">

    <ui:with field='messages' type='config.i18n.client.ClientMessages'/>
    <g:FlowPanel>
        <g:FlowPanel addStyleNames="wrapper skip_padding browse_level_dip">
            <common:NavigationToolbar ui:field="navigationToolbar" header="{messages.catalogueItemTitle}" />
            <g:FlowPanel addStyleNames="row full_width skip_padding">
                <g:FlowPanel addStyleNames="col_12 content">
                    <!-- IDENTIFICATION -->
                    <common:TitlePanel ui:field="title" addStyleNames="aipTitle">
                        <g:Label addStyleNames="browseItemId" ui:field="dateCreatedAndModified"/>
                    </common:TitlePanel>
                    <!-- DESCRIPTIVE METADATA -->
                    <g:FlowPanel ui:field="preMetadata"/>
                    <g:FlowPanel addStyleNames="descriptiveMetadataCard">
                        <g:HTML ui:field="descriptiveMetadata" addStyleNames="browseItemMetadata"/>
                    </g:FlowPanel>

                    <wcag:AccessibleFocusPanel ui:field="keyboardFocus">
                        <g:FlowPanel styleName="viewRepresentationFile" ui:field="container">
                            <g:FlowPanel addStyleNames="viewRepresentationFile-center" ui:field="center"/>
                        </g:FlowPanel>
                    </wcag:AccessibleFocusPanel>
                </g:FlowPanel>
            </g:FlowPanel>
        </g:FlowPanel>
    </g:FlowPanel>
</ui:UiBinder>
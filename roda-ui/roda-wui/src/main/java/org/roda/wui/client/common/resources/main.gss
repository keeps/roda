code {
    padding: .2rem .5rem;
    margin: 0 .2rem;
    font-size: 90%;
    white-space: nowrap;
    background: #F1F1F1;
    border: 1px solid #E1E1E1;
    border-radius: 4px;
}

pre > code {
    display: block;
    padding: 1rem 1.5rem;
    white-space: pre-wrap;
    overflow: scroll;

    /* Word break in FF */
    overflow-wrap: break-word;
    word-wrap: break-word;
    -ms-word-break: break-all;
    word-break: break-all;
    /* @alternate */ word-break: break-word;
}

pre code {
    max-height: 15em;
}



.code-pre {
    display: block;
    padding: 1rem 1.5rem;
    white-space: pre-wrap;
    overflow: auto;
    font-size: 90%;
    background: #F1F1F1;
    border: 1px solid #E1E1E1;
    border-radius: 4px;
}

.error {
    color: #D20707;
}

.error:before {
    content: '\f071';
    @mixin fontawesome();

    padding-left: 5px;
    margin-right: 5px;
}

.success {
    color: #24c279;
}

.success:before {
    content: '\f00c';
    @mixin fontawesome();

    padding-left: 5px;
    margin-right: 5px;
}

.error pre > code {
    color: black;
}


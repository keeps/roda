  RODA 6 cards
 ******************************/

.roda6Card {
    background-color: white;
    border-radius: 8px;
    box-shadow: 0px 2px 4px 0px COLOR_SHADOW;
    display: flex;
    flex-direction: column;
    padding: 16px;
}

.roda6CardWithHeader {
    background-color: white;
    border-radius: 8px;
    box-shadow: 0px 2px 4px 0px COLOR_SHADOW;
    display: flex;
    flex-direction: column;
}

.roda6CardWithHeader .cardHeader {
    background-color: COLOR_GREY_VERYLIGHTEST;
    padding: 8px 16px;
    border-radius: 8px 8px 0px 0px;
}

.roda6CardWithHeader .cardBody {
  padding: 8px 16px;
}

.gwt-DialogBox .roda6Card,
.gwt-DialogBox .roda6CardWithHeader {
  box-shadow: none;
}

/******************************
  RODA 6 buttons
 ******************************/

 .linkButton {
    font-size: 14px;
    font-weight: bold;
    border: none;
    background-color: transparent;
    color: COLOR_PRIMARY;
    text-transform: uppercase;
    display: flex;
    gap: 8px;
 }

 .linkButton:hover {
    cursor: pointer;
    border-bottom: 1px solid COLOR_PRIMARY;
 }



/******************************
  Plugin info
 ******************************/

.licenseLabel {
    float: left;
    margin-right: 2px;
    font-weight: bold;
}

.plugin-status-panel {
    background-color: #e0e0e0;
    padding: 8px;
    display: flex;
    align-items: center;
    margin-bottom: 1rem;
    border-radius: 2px;
    margin-top: 3px;
}

.plugin-status-message {
    margin-left: .5em;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.certificateStatus {
    margin-left: 10px;
}

.pluginWorkflowListTitle {
    display: flex;
    flex-wrap:wrap;
    justify-content: space-between;
}

.pluginWorkFlowListTitleButtons {
    color: #7f7f7f;
    margin-left: 5px;
    background-color: #e0e0e0;
    align-self: center;
}

.pluginWorkFlowListTitleButtons:hover {
    color: #fff;
}

.plugin-status-btn {
    margin-left: auto;
}

.plugin-categories-panel {
    padding-bottom: 10px;
}

.plugin-categories-panel .badge-panel {
    margin-right: 1rem;
}

.plugin-install-btn {
    min-width: 95px;
}

/******************************
  Common Tabs
 ******************************/
.tabs {
    padding: 20px 0;
    width: 100%;
}

.tabs .gwt-TabBar {
    line-height: 3.5rem;
    width: auto !important; /* needed so buttons appear on the right */
    border-collapse: collapse;
}

.tabs .gwt-TabPanelBottom {
    background-color: transparent;
    min-height: 25px;
    padding: 0px;
}

.tabs .gwt-TabBarItem {
    background: transparent;
    border: none;
    margin: 0;
    cursor: pointer;
    cursor: hand;
    font-size: 15px;
    padding: 0;
    position: relative;
    color: #999;
    border-bottom: solid 2px transparent;
}

.tabs .gwt-TabBarItem:hover {
    color: COLOR_PRIMARY;
    border-bottom: solid 2px COLOR_PRIMARY;
}

.tabs .gwt-TabBarItem-selected {
    margin: 0;
    border-bottom: solid 2px COLOR_PRIMARY;
    color: COLOR_PRIMARY;
}

.tabs .gwt-TabBarItem .gwt-Label {
    padding-left: 10px;
    padding-right: 10px;
    color: inherit;
    font-weight: 300;
    font-size: 1.5rem;
}


/******************************
  Badges
 ******************************/
 .badge-panel {
    position: relative;
    background-color: #e0e0e0;
    display: inline-flex;
    padding: 0.7rem;
    line-height: 1;
    color: #7f7f7f;
    text-align: center;
    white-space: nowrap;
    vertical-align: baseline;
    border-radius: .25em;
    align-items: center;
}

.badge-label {
    font-family: FONT_PRIMARY;
    font-size: 1.1rem;
    text-align: left;
    text-transform: uppercase;
    text-decoration: none;
}

.badge-panel-dark {
    background-color: #2b303a;
    color: #fff;
}

.badge-panel-success {
    background-color: COLOR_LABEL_SUCCESS;
    color: #fff;
}

.badge-panel-danger {
    background-color: COLOR_LABEL_DANGER;
    color: #fff;
}

.badge-icon {
    margin-right: .5em;
    font-size: 1.1rem;
    width: 1.5rem;
}

.badge-icon span.fa-stack {
    line-height: 1;
    height: 1rem;
    vertical-align: baseline;
    width: inherit;
}

.badge-for-version {
    border-radius: 2px;
    border: none;
    padding: 1rem;
    margin-left: 5px;
    background-color: #c5eafc;
    align-self: center;
    cursor: pointer;
}

.badge-for-version .badge-label {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 60px;
    font-weight: 600;
    font-size: 1.0rem;
    text-align: left;
    color: #286b93;
}

.badge-notification {
    color: #286b93;
    font-size: 0.8rem;
    height: 0.8rem;
    line-height: 0.8rem;
    padding-left: 2px;
    width: 1.5rem;
}

.badge-for-version .badge-notification .fa-stack {
    font-size: inherit;
    height: inherit;
    line-height: inherit;
}

.badge-for-version .badge-notification i {
    vertical-align: middle;
    font-size: inherit;
}

.badge-for-version .badge-notification i:first-child {
    font-size: 1.6em;
}

.badge-for-version .badge-notification i:last-child {
    font-size: 0.8em;
}



.collapsable-card-panel-close {
  background-color:transparent;
  border: none;
  right: 20px;
  position: absolute;
  color: #555;
  cursor: pointer;
  top: 20px;
}

.ingest-tab-panel {
    border: 1px solid #ffe69c;
    padding: 7px 10px;
    display: flex;
    align-items: center;
    margin-bottom: 1rem;
    border-radius: 2px;
    font-weight: 200;
    font-size: 1.3rem;
    background-color: #FFF3CD;
    color: #664d03;
}

.ingest-tab-panel-label {
    padding: 3px 5px;
}

.ingestTabPanelclose{
    background-color: transparent;
    border: none;
    right: 20px;
    color: #555;
    cursor: pointer;
    top: 20px;
    float:right;
    margin-left:auto;
}

.ingest-tab-panel .ingestTabPanelclose:hover {
  color: black;
}

.ingest-tab-panel a {
  font-weight: bold;
}

.conversion-profile {
    display: flex;
    align-items: center;
    margin-bottom: 5px;
}

.conversion-profile select {
    margin-right: 10px
}

.value-overflow {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 69vw;
}



.wui-dialog-license-layout-footer {
     text-align: right;
     margin-top: 10px;
     margin-bottom: 5px;
 }

 /******************************
   Custom Material Icons
  ******************************/

.cmi {
    font-family: 'CustomMaterialIcons';
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-weight: 900;
    font-style: normal;
}

.cmi-accountTree:after {

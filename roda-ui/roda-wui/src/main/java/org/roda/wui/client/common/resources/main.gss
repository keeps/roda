}

.inline {
    display: inline;
}

.text-align-left {
	text-align: left;
}

.text-align-right {
	text-align: right;
}

div.inline {
    display: inline-block;
}

.inline + .inline {
    margin-left: 0.5em;
}

.fade-out {
    white-space: nowrap;
    position: relative;
    overflow: hidden;
}

.fade-out:after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    width: 2em;
    height: 100%;
    /* @alternate */ background: -webkit-linear-gradient(right, #fff 30%, rgba(255, 255, 255, 0) 100%);
    background: linear-gradient(-90deg, #fff 30%, rgba(255, 255, 255, 0) 100%);
}

.my-asyncdatagrid-side-panel .fade-out:after {
    /* @alternate */ background: eval('"-webkit-linear-gradient(-90deg," + org.roda.wui.common.client.tools.ConfigurationManager.getString("ui.css.color.table.sidebar.background") +  " 30%, rgba(255, 255, 255, 0) 100%)"');
    background: eval('"linear-gradient(-90deg," + org.roda.wui.common.client.tools.ConfigurationManager.getString("ui.css.color.table.sidebar.background") +  " 30%, rgba(255, 255, 255, 0) 100%)"');
}

.sidebar .fade-out:after {
    /* @alternate */ background: eval('"-webkit-linear-gradient(-90deg," + org.roda.wui.common.client.tools.ConfigurationManager.getString("ui.css.color.sidebar.background") +  " 30%, rgba(255, 255, 255, 0) 100%)"');
    background: eval('"linear-gradient(-90deg," + org.roda.wui.common.client.tools.ConfigurationManager.getString("ui.css.color.sidebar.background") +  " 30%, rgba(255, 255, 255, 0) 100%)"');
}


.card {
    background-color: white;
    box-shadow: 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23);
    transition: all 0.3s cubic-bezier(.25,.8,.25,1);
    border-radius: 2px;
    padding: 15px;
}

.card:hover {
    box-shadow: 0 10px 20px rgba(0,0,0,0.19), 0 6px 6px rgba(0,0,0,0.23);
}

.cardInvisible {
    background-color: transparent;
    box-shadow: none;
    transition: none;
    border-radius: 0;
    padding: 0;
}

.cardInvisible:hover {
    box-shadow: none;
}

.card + .card {
    margin-top: 30px;
}

.card > .h5,
.card .actionable-header,
.representationInformationSubtitle{
    margin-top: 0;
    color: #999;
    font-weight: 300;
    margin-bottom: 1rem;
}

.filesCard > .h5:before {
    content: '\f0c5';
    padding-right: 10px;
    @mixin fontawesome();
}

.disseminationsCard > .h5:before {
    content: '\f144';
    padding-right: 10px;
    @mixin fontawesome();
}

.representationInformationCard {
    margin-top: 10px;
}

.representationInformationCardCreate > .h5 {
    color: inherit;
}

.representationInformationCard > .h5:before {
    content: '\f05a';
    @mixin fontawesome();
    padding-right: 10px;
    color: COLOR_PRIMARY;

}

.verticalDivider {
    width: 1px;
    background-color: COLOR_GREY_LIGHT;
}

.sidebarActionsDivider {
  height: 1px;
  background-color: #e5e5e5;
  width: 75%;
  margin-top: 10px;
  margin-bottom: 20px;
}

.browse {
    background-color: COLOR_GREY_VERYLIGHT;
}

.browse_top .aipChildrenCard {
	box-shadow: none;
	padding: 0;
	transition: none;
}

.browse_top .aipChildrenCard .actionable-header-focus-panel {
    display: none;
}

.browse_top .aipChildrenCard:hover {
	box-shadow: none;
}

.browse_aip .aipChildrenCard {
    box-shadow: none;
    padding: 0;
    transition: none;
}

.browse_aip .aipChildrenCard:hover {
    box-shadow: none;
}

.browse_aip .row .content {
    padding: 0!important;
    width:100%;
}

.browse_aip .row .content .upperContent {
    padding: 30px;
}

.browse_aip .row .content .lowerContent {
    padding: 30px;
    background-color: COLOR_GREY_BGDARKER;
}

.browse_aip .row .content .lowerContent .card {
    background-color: transparent;
}creature

.browse_aip .row .content .lowerContent .card .searchWrapper .sidebar-facet-label.fade-out:after {
        background: linear-gradient(-90deg, COLOR_GREY_BGDARKER 30%, rgba(255, 255, 255, 0) 100%);
}

.descriptiveMetadataCard {
    padding: 0;
}

.slider-container {
	position: relative;
}

.slider {
    height: 100%;
    width: 380px;
    background: COLOR_PRIMARY;
    display: none;
    color: white;
    position: absolute;
    right: 0;
    top: 0;
    padding: 0;
    box-shadow: 0 0 0 0 transparent, 0 0 0 0 transparent, 0 0 0 0 transparent, -6px 0 15px -4px rgba(31, 73, 125, 0.8);
    z-index: 1;
}

.slider-title, .slider .actions-layout-title {
    font-size: 2.2rem;
    margin: 1.2rem 0;
}

.slider-title {
	margin-top: 20px;
    text-transform: capitalize;
}

.slider .slider-layout {
	opacity: 0;
	transition: opacity 0.25s ease-in-out;
	overflow-y: auto;
    position: relative;
    zoom: 1;
    height: 100%;
    overflow-x: hidden;
    padding-left: 25px;
    padding-right: 25px;
}

.slider-active .slider-layout {
	opacity: 1;
}

.my-asyncdatagrid-display th input {
    /* header checkbox in AsyncTableCell */
    margin-left: 6px;
}


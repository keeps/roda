    display: none;
}

#upload-list {
    list-style: none;
}

#upload-list li {
    height: 52px;
    position: relative;
}

#upload-list li input {
    display: none;
}

#upload-list li p {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    font-size: 1.3rem;
    font-weight: 500;
    position: absolute;
    top: 10px;
    left: 60px;
}

#upload-list li i {
    font-weight: normal;
    font-style: normal;
    color: #7f7f7f;
    display: block;
    font-size: 1.1rem;
}

#upload-list li canvas {
    top: 15px;
    left: 20px;
    position: absolute;
}

#upload-list li span.icon:after {
    display: inline-block;

    font-size: 2rem;
    line-height: 2rem;
    width: 2rem;
    height: 2rem;

    content: "\e876";
    @mixin fontawesome();

    position: absolute;
    top: 20px;
    right: 20px;
    cursor: pointer;

    color: #24c279;
    -webkit-animation: none;
    -moz-animation: none;
    animation:none;
}

#upload-list li.working span.icon:after {
    content: "\e1af";
    color: #f4d03f;
    -webkit-animation:spin 3s linear infinite;
    -moz-animation:spin 3s linear infinite;
    animation:spin 3s linear infinite;
}
@-moz-keyframes spin { 100% { -moz-transform: rotate(360deg); } }
@-webkit-keyframes spin { 100% { -webkit-transform: rotate(360deg); } }
@keyframes spin { 100% { -webkit-transform: rotate(360deg); transform:rotate(360deg); } }

#upload-list li.error p {
    color: #D20707;
}

#upload-list li.error span.icon:after {
    content: "\e002";
    color: #D20707;
    -webkit-animation: none;
    -moz-animation: none;
    animation:none;
}
#upload-list li.error:before {
    content: "";
}

#upload-list li.error .errorMessage {
	margin-left: 20px;
}



    margin-bottom: 0.1rem;
    display: block;
}

.file .header {
    display: block;
    font-size: 1.4rem;
    display: block;
}

.file .header:before {
    content: '\f1b2';
    @mixin fontawesome();
    margin-right: 5px;
}

.file .header .identifiers {

}


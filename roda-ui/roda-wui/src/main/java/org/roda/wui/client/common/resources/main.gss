    min-width: 150px;
    cursor: pointer;
    text-transform: capitalize;
}

.dropdown-disabled .dropdown-label,
.dropdown-disabled .dropdown-item {
    cursor: default;
}

.dropdown-item:hover {
    color: white;
    background-color: black;
}

.dropdown-label:after {
    display: inline-block;

    content: "\e5cf";
    @mixin fontawesome();

    font-size: inherit;

    float: right;
    line-height: 2.5rem;
    padding-left: 5px;
    padding-right: 5px;
}

.dropdown-disabled .dropdown-label:after {
   display: none;
}


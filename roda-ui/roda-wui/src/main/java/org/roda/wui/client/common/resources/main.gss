/**********************************************
				COMMON
************************************************/

.html, html {
    font-size: 62.5%;
}

.body, body {
    font-size: 1.5em;
    line-height: 1.6;
    font-weight: 400;
    font-family: FONT_PRIMARY;
    color: #222;
}

.roda {
    display: flex;
    flex-direction: column;
    background-color: COLOR_FOOTER_BACKGROUND;
    min-height: 100vh;
}

a {
    text-decoration: none;
    color: COLOR_PRIMARY;
}

a:HOVER {
	text-decoration: underline;
}

select {
    color: #444;
    padding: 5px;
    line-height: 1.6;
    font-weight: 400;
    font-family: FONT_PRIMARY;
    font-size: 1.3rem;
}

.not-visible {
    display: none;
}

.clickable {
    cursor: pointer;
}

h1, h2, h3, h4, h5, h6, .h1, .h2, .h3, .h4, .h5, .h6 {
    margin-top: 2rem;
    margin-bottom: 1rem;
    font-weight: 400;
}

h1, .h1 {
    font-size: 4.0rem;
    line-height: 1.2;
    letter-spacing: -.1rem;
}

h2, .h2 {
    font-size: 3.6rem;
    line-height: 1.25;
    letter-spacing: -.1rem;
}

h3, .h3 {
    font-size: 3.0rem;
    line-height: 1.3;
    letter-spacing: -.1rem;
}

h4, .h4 {
    font-size: 2.4rem;
    line-height: 1.35;
    letter-spacing: -.08rem;
}

h5, .h5 {
    font-size: 1.8rem;
    line-height: 1.5;
    font-weight: 600;
}

h6, .h6 {
    font-size: 1.5rem;
    line-height: 1.6;
    letter-spacing: 0;
}

.noMargin {
    margin: 0px;
}

.rodaTitleHeader i {
    margin-right: 8px;
}

ul {
    list-style: inherit;

}

.cmi-file:after {
    content: "\e911"
}

.cmi-envelope:after {
    content: "\e928"
}

.cmi-circle-plus:after {
    content: "\e902";
}

.largeIcon {
    font-size: 20px;
}

.cmi-user:after {
    content: "\e922";
}

.cmi-password:after {
    content: "\e941";
}

.cmi-enter:after {
    content: "\e92c";
}

.details-date {
    font-size: 1.1rem;
    color: #999;
    font-style: italic;
}

.details-uuid {
    font-size: 1.6rem;
    color: rgb(88, 89, 91);
    font-family: FONT_MONOSPACED;
}

.details-anchor {
    display: block;
    margin-bottom: 0.5em;
}
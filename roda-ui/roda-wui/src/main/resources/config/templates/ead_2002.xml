<?xml version="1.0" encoding="UTF-8"?>
<ead
        xmlns="urn:isbn:1-931666-22-9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="urn:isbn:1-931666-22-9 ../schemas/ead2002.xsd">
    <eadheader>
        <eadid>
        </eadid>
        <filedesc>
            <titlestmt>
                <titleproper>{{title order='2' auto-generate='title'}}
                </titleproper>
            </titlestmt>
        </filedesc>

        <profiledesc>
            <creation>
                Generated by RODA version 2.0
                <date normal="{{descDate order='3' type='date' auto-generate='now'}}">{{descDate}}</date>
            </creation>
        </profiledesc>

    </eadheader>
    <archdesc
            level='{{level type="list" list="[\"fonds\",\"subfonds\",\"series\",\"subseries\",\"file\",\"item\"]" auto-generate="level" order="6"}}'>
        <did>
            <unittitle>{{title}}</unittitle>
            <unitid>{{id order="1" auto-generate="id"}}</unitid>
            {{#if dateInitial}}
            {{#if dateFinal}}
            <unitdate normal="{{dateInitial order='4' type='date'}}/{{dateFinal order='5' type='date'}}">
                {{dateInitial}}/{{dateFinal}}
            </unitdate>
            {{else}}
            <unitdate normal="{{dateInitial}}">{{dateInitial}}</unitdate>
            {{/if}}
            {{else}}
            {{#if dateFinal}}
            <unitdate normal="{{dateFinal}}">{{dateFinal}}</unitdate>
            {{/if}}
            {{/if}}
            <origination label="producer">{{producer}}</origination>
            <origination label="creator">{{creator}}</origination>
            <langmaterial>{{language auto-generate='language'}}</langmaterial>
        </did>
        <scopecontent>
            <p>{{description type='textarea'}}</p>
        </scopecontent>
        <userestrict>
            <p>{{rights}}</p>
        </userestrict>
    </archdesc>
</ead>
